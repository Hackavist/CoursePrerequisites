<Window x:Class="Course_Prerequsites_WPF.UIs.CourseSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Course_Prerequsites_WPF.UIs"
        mc:Ignorable="d"
        Title="CourseSettings" Height="650" Width="600" Background="White">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="104*"/>
            <ColumnDefinition Width="489*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="AddCourse" Content="Add/Edit Course" VerticalAlignment="Top" Margin="5,147,105.333,0" Height="88" TabIndex="1" FontSize="20" FontFamily="Times New Roman" Foreground="White" Background="#FFF6A10C" Grid.Column="1" Click="AddCourse_Click"/>
        <Button x:Name="ViewStudentsInCoursebtn" Content="View All Students In a Course&#xD;&#xA;" VerticalAlignment="Top" Margin="5,363,105.333,0" Height="88" TabIndex="1" FontSize="20" FontFamily="Times New Roman" Foreground="White" Background="#FFF6A10C" Grid.Column="1" Click="ViewStudentsInCourse_Click"/>

    </Grid>
</Window>
